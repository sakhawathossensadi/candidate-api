{
    "name": "analyzen/candidate",
    "description": "This is candiadte package",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sakhawathossensadi/auth-api.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Analyzen\\Candidate\\": "src/",
            "Analyzen\\Candidate\\Database\\Factories\\": "database/factories/",
            "Analyzen\\Candidate\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel" : {
            "providers": [
                "Analyzen\\Candidate\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "sakhawathossensadi",
            "email": "newtonss919@gmail.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Analyzen\\Candidate\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "minimum-stability": "dev",
    "require": {
        "analyzen/auth": "dev-master"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter",
        "test-w": "vendor\\bin\\phpunit",
        "test-wf": "vendor\\bin\\phpunit --filter"
    }
}
